    <thead>
      <tr>
        <th scope="col">ID</th>
       
        <th scope="col">Appointment_id</th>
        <th scope="col">Doctor_id</th>
        <th scope="col">Patient_id</th>
         <th scope="col">Prescription</th>
      </tr>
    </thead>
